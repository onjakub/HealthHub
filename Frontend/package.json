{
  "name": "healthhub-frontend",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --port 3000 --hostname localhost",
    "prebuild": "npm install",
    "build": "next build && rm -rf ../HealthHub/wwwroot/* && cp -R out/* ../HealthHub/wwwroot/",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf .next out",
    "clean:all": "rm -rf .next out node_modules package-lock.json",
    "install:clean": "npm run clean:all && npm install",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "graphql": "^16.8.1",
    "lucide-react": "^0.292.0",
    "next": "^15.1.4",
    "postcss": "^8.4.31",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9",
    "eslint-config-next": "^15.1.4",
    "typescript": "^5"
  }
}
